import java.util.*;
import java.util.Scanner;
public class Array1 {
    public static void main(String[] args) {
      
 
      Scanner sc = new Scanner(System.in);
      int size,i;
      System.out.println("enter array size:");
      size = sc.nextInt();
      int arr[];
      arr=new int[size];
      System.out.print("enter array elements:");
      for(i=0;i<size;i++)
      {
        arr[i]=sc.nextInt();
      }
      Arrays.sort(arr);
      System.out.println("minimum elements is:" +arr[0]);
      System.out.println("maxium elements is:" +arr[size-1]);
    }
      }
